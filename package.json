  "type": "module",
  "scripts": {
    "audit": "npx upgradeps -v",
    "clean": "npx rimraf node_modules && npx rimraf package-lock.json",
    "lint": "npx prettier --write . && npx standard --fix",
    "nuke": "npm run clean && npm cache verify && npm install",
    "sort": "npx sort-package-json",
