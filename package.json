    "date-fns": "2.29.3",
    "figures": "5.0.0",
    "jsonfile": "6.1.0",
    "lodash": "4.17.21",
    "strip-ansi": "7.0.1",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "prettier": "2.8.7",
    "rimraf": "4.4.1",
    "sort-package-json": "2.4.1",
    "standard": "17.0.0",
    "upgradeps": "2.0.5"
  }
